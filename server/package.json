    "db:setup": "yarn install && yarn db:create && yarn db:migrate"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.2",
    "express-session": "^1.17.1",
    "glob": "^7.1.3",
    "lodash": "^4.17.11",
    "morgan": "^1.9.0",
    "pg": "8.4.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "6.3.5",
    "sequelize-cli": "6.2.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.5"
  }
}
