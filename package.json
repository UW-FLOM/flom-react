{
  "scripts": {
    "setup": "yarn && cd client && yarn && cd ../server && yarn",
    "prod:setup": "yarn setup && yarn prod:start",
    "prod:start": "docker-compose build && docker-compose up -d",
    "prod:resume": "docker-compose up -d",
    "prod:stop": "docker-compose stop",
    "prod:destroy": "docker-compose down -v",
    "dev:setup": "yarn setup && yarn dev:start",
    "dev:start": "docker-compose -f docker-compose.dev.yml build && docker-compose -f docker-compose.dev.yml up -d",
    "dev:resume": "docker-compose -f docker-compose.dev.yml up -d",
    "dev:stop": "docker-compose -f docker-compose.dev.yml stop",
    "dev:destroy": "docker-compose -f docker-compose.dev.yml down -v"
  },
  "dependencies": {},
  "license": "UNLICENSED",
  "devDependencies": {
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2"
  }
}
